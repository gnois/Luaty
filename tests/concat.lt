var foo = \a, flag->
	return a .. (flag and 'boom' or 'pst')

assert('ciaoboom' == foo('ciao', true))
assert('ciaopst' == foo('ciao', false))

var bar = \a, b, c->
	return a .. b .. c

assert(bar('ciao', 'ciccio', 'boum') == 'ciaociccioboum')


