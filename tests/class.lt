var BaseClass = \base->
	var private = base

	var @ = {
		foo = ->
			return private
	}
	return @


var DerivedClass = \base, derived->
	var private = derived

	var @ = BaseClass(base)
	@.public = derived

	-- overrride base foo()
	var foo_ = @.foo
	@.foo = ->
		return private + @.public + foo_()

	return @


var i = DerivedClass(1, 2)
print(i.foo()) --> 5
