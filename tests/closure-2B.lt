var make = \p->
	if p > 10
		var n = p + 1
		return {
			incr = \i->
				n = n + i
			, mult = \i-> 
				n = n * i
			, get = -> 
				return n
		}
	else
		var n = 2*p
		return {
			incr = \i-> 
				n = n + i
			, mult = \i-> 
				n = n * i
			,get = ->
				return n
		}

		
var obj = make(7)
obj.mult(3)
obj.incr(1)
obj.mult(2)
obj.incr(5)
print(obj.get())
