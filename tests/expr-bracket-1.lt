var foo = \x->
	return x, x + 1, 2*x

var prova = \...->
	return ...

var one = \...->
	return (...)

var single_value = \x->
	return (foo(x))

var alls = \x->
	return foo(x)

print(foo(2))
print( (foo(2)) )

print(alls(foo(2)))
print(single_value(foo(2)))

print( one(foo(2)) )

print( prova(foo(2)) )
