-- just test
var foo = \n->
	var sum = 0
	var fs = {}
	for i = 1, n 
		sum = sum + i*i
		fs[i] = \x->
			return i + x 
	return sum, fs


var s, fs = foo(10)
print(s)
print(fs[1](7), fs[2](7), fs[3](7))



